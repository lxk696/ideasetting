<templateSet group="lxk-customer">
  <template name="syso" value="System.out.println($EXPR$);$END$" description="syso输出" toReformat="false" toShortenFQNames="true">
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_STRING" value="true" />
    </context>
  </template>
  <template name="cc" value="/**&#10; * @Date: $DATE$ $TIME$&#10; * @Param: $PARAMS$&#10; * @Return: $RETURN$&#10; * @author: lxk&#10; */" shortcut="ENTER" description="方法注释" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="*" value="*&#10; * @Description:&#10; * @Author: lxk &#10; * @Date: $DATE$ $TIME$&#10; * @Param: $PARAMS$&#10; * @Return: $RETURN$&#10;*/" shortcut="ENTER" description="方法注释" toReformat="false" toShortenFQNames="true">
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="llist" value="List&lt;String&gt; list = new LinkedList&lt;&gt;();" description="生成linkedList" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="list" value="List&lt;String&gt; list = new ArrayList&lt;&gt;();" description="生成arraylist" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="map" value="java.util.Map&lt;String,Object&gt; map = new java.util.HashMap&lt;&gt;(16); " description="生成hashmap" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="log" value="private static final org.slf4j.Logger logger = org.slf4j.LoggerFactory.getLogger($CLASS_NAME$.class);&#10;$END$&#10;" description="生成slf4j" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lpm2" value="logger.$TYPE$($params$);" description="打印方法入参" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="groovyScript(&quot;'\&quot;' + _1.collect { it + ' = [\&quot; + ' + it + ' + \&quot;]'}.join(', ') + '\&quot;'&quot;, methodParameters())" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lpm" value="logger.$TYPE$(&quot;$msgss$  $paramsPlaceholder$&quot; ,new Object[]{$params$});" description="slfj参数打印" toReformat="false" toShortenFQNames="true">
    <variable name="TYPE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="msgss" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="paramsPlaceholder" expression="groovyScript(&quot;_1.collect { it + ' = [{}]'}.join(', ')&quot;, methodParameters());" defaultValue="" alwaysStopAt="false" />
    <variable name="params" expression=" groovyScript(&quot;_1.collect{it}.join(',')&quot;, methodParameters())" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="lptest" value=" logger.info(new Object[]{$params$}.toString());" description="test" toReformat="false" toShortenFQNames="true">
    <variable name="params" expression="groovyScript(&quot;_1.collect { it }.join(', ')&quot;, methodParameters());" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="sb" value="StringBuilder sb = new StringBuilder();" description="sb" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="time" value="long startTime = System.currentTimeMillis();&#10;logger.$type$(&quot;$msgs$ cost time is [{}]&quot;, System.currentTimeMillis() - startTime);" description="耗时日志" toReformat="false" toShortenFQNames="true">
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="msgs" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="logerr" value="log.error(e.getMessage(), e);" description="打印错误日志" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>